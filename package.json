{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with VanillaJS and NodeJS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dispellizer/wetube.git"
  },
  "author": "Dispellizer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dispellizer/wetube/issues"
  },
  "homepage": "https://github.com/Dispellizer/wetube#readme",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.1.5",
    "@babel/runtime": "^7.8.7",
    "autoprefixer": "^9.7.4",
    "aws-sdk": "^2.642.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.0",
    "helmet": "^3.21.2",
    "mongoose": "^5.9.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "ngrok": "^3.2.7",
    "node-sass": "^4.13.1",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "passport-naver": "^1.0.6",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "sass-loader": "^8.0.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "tunnel": "ngrok http 4000",
    "start": "node build/init.js",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "nodemon": "^1.18.6",
    "prettier": "^1.15.2"
  }
}
